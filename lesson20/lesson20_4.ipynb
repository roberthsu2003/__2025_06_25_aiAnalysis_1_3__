    "import numpy as np\n",
    "import pandas as pd\n",
    "\n",
    "tips = pd.read_csv('tips.csv')\n",
    "tips.columns = ['總票價','小費','吸煙者','日期','時間','大小']\n",
    "tips['小費比例'] = tips['小費'] / tips['總票價']\n",
    "\n",
    "#將小費比例轉換為`xx.x%`\n",
    "tips['小費比例'] = tips['小費比例'].map(\"{:.1%}\".format)\n",
    "tips.head()"
   ]
  }
